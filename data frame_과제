id <- c(10,20,30,40,50)
name <- c("John","Tom","Paul","Jane","Grace")
score <- c(95,46,98,74,85)
df <- data.frame(id,name,score)

df

#or

df <- data.frame(id=c(10,20,30,40,50),name=c("John","Tom","Paul","Jane","Grace"),score=c(95,46,98,74,85))
----------------------------------------------------------
  
df

df$score
df[,3]
df[,'score']

----------------------------------------------------------

df
df[,c(1,3)]

#or
df[,c("id","score")]

----------------------------------------------------------
  
df[c(2,3),]
df[2:3,]

----------------------------------------------------------
df[2,3]
